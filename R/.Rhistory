visibility.readSpectrum <- function(inputFile) {
filePath = sprintf("spectra/%s", inputFile)
inputSpectrum = read.csv(filePath) # 2 columns labeled 'energy' [keV] and 'photons'
# Normalize
inputSpectrum$photons = inputSpectrum$photons/sum(inputSpectrum$photons)
return(inputSpectrum)
}
geometry.calcWavelength <- function(designEnergy)  {
# designEnergy in [keV]
# returns wavelength in [um]
h = 6.62606957e-34 # [Js]
c = 299792458 # [m/s]
designEnergy = designEnergy * 1e3 * 1.602176565e-19 # [keV -> eV -> J]
lambda = h * c / designEnergy # [m]
lambda = lambda * 1e6 # [um]
return(lambda)
}
